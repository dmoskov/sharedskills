{
  "family": "discord-bots",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["EC2", "FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::181691141781:role/crucible-remote-agent-analysis-execution-role",
  "taskRoleArn": "arn:aws:iam::181691141781:role/crucible-remote-agent-analysis-task-role",
  "containerDefinitions": [
    {
      "name": "discord-bots",
      "image": "181691141781.dkr.ecr.us-west-2.amazonaws.com/discord-bots:latest",
      "essential": true,
      "environment": [
        {"name": "AWS_SECRET_NAME", "value": "discord-bot-token"},
        {"name": "AWS_REGION", "value": "us-west-2"},
        {"name": "POLL_INTERVAL", "value": "60"}
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/discord-bots",
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
